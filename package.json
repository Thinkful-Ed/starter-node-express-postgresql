{
  "name": "starter-node-express-postgresql",
  "version": "1.0.0",
  "description": "Starter code for the Node.js, Express, and PostgreSQL module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
